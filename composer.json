{
    "name": "raj/laravel-files",
    "type": "library",
    "description": "Provides an easy to use api for uploading and manging all files in a laravel project",
    "keywords": [
        "raj", "raj-kaj", "this-is-raj", "Laravel", "Files", "Laravel Files", "LaravelFiles"
    ],
    "homepage": "https://github.com/this-is-raj/laravel_files",
    "license": "MIT",
    "authors": [
        {
            "name": "Raj Kumar",
            "email": "rajkjjn@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": ">=5.3.23",
        "guzzlehttp/guzzle": "^5.3.1|^6.2.1",
        "illuminate/support": "~5.1",
        "aws/aws-sdk-php-laravel": "~3.0",
        "aws/aws-php-sns-message-validator": "^1.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "raj\\MailTracker\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "jdavidbakr\\MailTracker\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Raj\\LaravelFiles\\LaravelFileServiceProvide"
            ]
        }
    }
}
